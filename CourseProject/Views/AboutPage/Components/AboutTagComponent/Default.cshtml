@model List<CourseProject.Models.Entity.AboutTag>

<div class="container">
    <div class="row">
        <div class="col-lg-6 offset-lg-3">
            <div class="section-heading text-center">
                @foreach (var item in Model)
                {
                    <h2>@item.Title</h2>
                    <p>@item.Description</p>
                }
            </div>
        </div>
    </div>
</div>